<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.int10.belt">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Belt"
        tools:targetApi="31">
        <activity
            android:name=".BeltActivity"
            android:exported="false" />
        <activity
            android:name=".ModelActivity"
            android:exported="false" />
        <activity
            android:name=".SamBeltDetailActivity"
            android:exported="false" />
        <activity
            android:name=".SamBeltModelActivity"
            android:exported="false" />
        <activity
            android:name=".SamBeltActivity"
            android:exported="false" />
        <activity
            android:name=".EngineModelActivity"
            android:exported="false" />
        <activity
            android:name=".EngineSerialActivity"
            android:exported="false" />
        <activity
            android:name=".BeltSerialActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>